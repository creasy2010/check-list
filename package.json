{
  "name": "check-list-app",
  "version": "0.0.7-beta.14",
  "description": "A minimal Electron application written with Typescript",
  "main": "./lib/main.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:mac": "electron-builder build  --mac",
    "watch": "tsc -w",
    "prepublishOnly": "tsc",
    "open": "electron ./lib/main.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./lib/main.js"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-rebuild": "^1.8.8",
    "electron-builder": "^21.2.0",
    "electron": "^7.1.9",
    "@types/fs-extra": "^5.0.5",
    "@types/klaw": "^3.0.0",
    "@types/uuid": "^3.4.6",
    "jest": "^24.7.1",
    "prettier": "1.16.4",
    "tslint": "^5.10.0"
  },
  "dependencies": {
    "@types/request": "^2.48.1",
    "@types/urllib": "^2.33.0",
    "ejs": "^2.6.1",
    "fs-extra": "^8.1.0",
    "generate-schema": "^2.6.0",
    "json-schema-to-typescript": "^6.1.3",
    "jsonschema": "^1.2.4",
    "klaw": "^3.0.0",
    "request": "^2.88.0",
    "urllib": "^2.34.1",
    "uuid": "^3.4.0"
  },
  "build": {
    "productName": "checklist",
    "appId": "com.dong.checklist",
    "copyright": "checkoist",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "icon.png",
      "extraResources": [
        "./dist/**/*.exe",
        "./dist/**/*.dll",
        "./dist/**/*.json",
        "./dist/**/*.png",
        "./dist/**/*.conf"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "dmg": {
      "contents": []
    },
    "linux": {
      "icon": "build/icons"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "checklist"
    },
    "files": [
      "lib/**/*",
      "static-web/**/*"
    ]
  },
  "gitHead": "9c201ea30833f5b90050e5d2eb887ef7a8977109"
}
